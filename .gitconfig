[user]
    name = Adrian Nicolau
    email = adrian.nicolau74@gmail.com
[core]
    editor = vim
[color]
    ui = true
[color "branch"]
    current = yellow bold
    local = green bold
    remote = cyan bold
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
    whitespace = red reverse
[color "status"]
    added = green bold
    changed = yellow bold
    untracked = red bold
[alias]
    pom = push origin master
    cm = commit -m
    s = status
    f = fetch
